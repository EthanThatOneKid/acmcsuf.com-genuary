{
  "name": "acmcsuf.com",
  "version": "3.0.0",
  "private": true,
  "homepage": "https://acmcsuf.com/",
  "author": {
    "name": "EthanThatOneKid",
    "url": "https://etok.codes/"
  },
  "repository": {
    "url": "https://github.com/EthanThatOneKid/acmcsuf.com.git/"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest --run && npm run test:browser",
    "test:watch": "vitest",
    "test:size": "npm i && size-limit",
    "test:coverage": "vitest run --coverage",
    "test:browser": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --compiler-warnings \"css-unused-selector:ignore,unused-export-let:error\"",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "host": "npm run all && npm run preview",
    "all": "npm run lint && npm run format && npm run check && npm run build && npm t"
  },
  "devDependencies": {
    "@playwright/test": "^1.36.2",
    "@size-limit/file": "^8.2.6",
    "@sveltejs/adapter-netlify": "next",
    "@sveltejs/kit": "next",
    "@types/html-to-text": "^9.0.1",
    "@types/qrcode": "^1.5.1",
    "@types/rss": "^0.0.30",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitest/coverage-c8": "^0.33.0",
    "devalue": "^4.3.2",
    "discord.js": "^14.12.1",
    "dotenv": "^16.3.1",
    "esbuild": "^0.18.17",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.3",
    "sass": "^1.64.2",
    "size-limit": "^8.2.6",
    "svelte": "^4.1.2",
    "svelte-check": "^3.4.6",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0"
  },
  "size-limit": [
    {
      "path": "node_modules",
      "limit": "256 MB"
    }
  ],
  "type": "module",
  "dependencies": {
    "@googleapis/calendar": "^9.1.0",
    "@js-temporal/polyfill": "^0.4.4",
    "html-to-text": "^9.0.5",
    "qrcode": "^1.5.3",
    "rrule": "^2.7.2",
    "rss": "^1.2.2",
    "svelte-confetti": "^1.2.4",
    "svelte-meta-tags": "^2.8.0"
  }
}
